---
permalink: /feed.xml
eleventyExcludeFromCollections: true
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{siteData.name}}</title>
    <link>{{siteData.url}}</link>
    <description>{{siteData.description}}</description>
    <language>en</language>
    <atom:link href="{{siteData.url}}/feed.xml" rel="self" type="application/rss+xml"/>
      {% for page in collections.articles %}
        <item>
          <title>{{page.title}}</title>
          <description>{{page.description}}</description>
          <pubDate>{{page.date | dateIso}}</pubDate>
          <link>{{siteData.url}}{{page.url}}</link>
          <guid isPermaLink="true">{{siteData.url}}{{page.url}}</guid>
       </item>
      {% endfor %}
  </channel>
</rss>
